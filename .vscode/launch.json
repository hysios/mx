    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Server",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/example",
            "cwd": "${workspaceFolder}",
            "args": [
                "-serve",
                "1"
            ]
        },
        {
            "name": "Launch Server 2",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/example",
            "cwd": "${workspaceFolder}",
            "args": [
                "-serve",
                "2"
            ]
        },
        {
            "name": "Launch Gateway",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/example",
            "cwd": "${workspaceFolder}",
            "args": [
                "-gateway",
            ]
        },
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/example",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Launch gen service",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd",
            "cwd": "${workspaceFolder}",
            "args": [
                "gen",
                "service",
                "--name",
                "test",
                "--pkg-name",
                "play.test",
                "-o",
                "tmp",
                "-m",
                "GetUser:GET:UserRequest{id:int32 name:string}:UserResponse{id:int32 name:string}"
            ]
        },
        {
            "name": "Launch config set",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd",
            "cwd": "${workspaceFolder}",
            "args": [
                "config",
                "set",
                "--key",
                "database.driver=mysql",
            ]
        },
        {
            "name": "gen add service",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd",
            "cwd": "${workspaceFolder}",
            "args": [
                "add",
                "service",
                "--name",
                "test",
                "--output",
                "_example"
            ]
        }
    ]
}